# Production Environment Variables for Build
# هذه القيم تستخدم فقط للبناء، القيم الحقيقية ستكون في Coolify

# Firebase Admin SDK (قيم مؤقتة للبناء)
FIREBASE_PROJECT_ID=temp-project-id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nTEMP_KEY\n-----END PRIVATE KEY-----"
FIREBASE_CLIENT_EMAIL=temp@temp.iam.gserviceaccount.com

# Firebase Public Config (قيم مؤقتة للبناء)
NEXT_PUBLIC_FIREBASE_API_KEY=temp-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=temp.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=temp-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=temp.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
NEXT_PUBLIC_FIREBASE_APP_ID=temp-app-id

# Supabase (قيم مؤقتة للبناء)
NEXT_PUBLIC_SUPABASE_URL=https://temp.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=temp-anon-key
SUPABASE_SERVICE_ROLE_KEY=temp-service-role-key

# App Settings
NODE_ENV=production
NEXT_PUBLIC_BASE_URL=https://temp.com 